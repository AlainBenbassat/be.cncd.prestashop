<?xml version="1.0"?>
<extension key="be.cncd.prestashop" type="module">
  <file>prestashop</file>
  <name>Import PrestaShop orders and customers</name>
  <description>Imports (synchronizes) order and customers from PrestaShop (boutique)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/AlainBenbassat/be.cncd.prestashop</url>
    <url desc="Documentation">https://github.com/AlainBenbassat/be.cncd.prestashop/blob/main/README.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-05-17</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Prestashop</namespace>
  </civix>
</extension>
